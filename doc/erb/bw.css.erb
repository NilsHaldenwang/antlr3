
.«lang» {
    width: 100%;
}
.«lang» table {
    border-collapse: collapse;
    border: 1px solid black;
    margin: 0px;
    padding: 0px;
    width: 100%;
}

.«lang» table .line_number {
    border-right: 1px solid black;
    border-left:  0px;
    border-top: 0px;
    border-bottom: 0px;
    margin: 0px;
    padding-left: 2px;
    padding-right: 2px;
    padding-top: 0px;
    padding-bottom: 0px;
    width: 2%;
}

.«lang» table .odd {
    background-color: #E8C681;
}

.«lang» table .even {
    background-color: #E8D2A7;
}

.«lang» pre {
    margin: 0px;
    padding: 0px;
    font-size: 12pt;
}

.«lang» table .code {
    margin: 0px;
    padding-left: 2px;
    padding-right: 2px;
    padding-top: 0px;
    padding-bottom: 0px;
    background-color: #ffffff;
}

.«lang» pre {
    margin: 0px;
    padding: 0px;  
}

