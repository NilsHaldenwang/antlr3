.«lang» { width: 100% }

.«lang»  table {
    border-collapse: collapse;
    border: 1px solid black;
    margin: 0px;
    padding: 0px;
    width: 100%;
}

.«lang» table .line_number {
    border-right: 1px solid black;
    border-left:  0px;
    border-top: 0px;
    border-bottom: 0px;
    margin: 0px;
    padding-left: 2px;
    padding-right: 2px;
    padding-top: 0px;
    padding-bottom: 0px;
    width: 2%;
    color: black;
    background-color: #000311;
}

.«lang» table .odd {
    background-color: #E8C681;
}

.«lang» table .even {
    background-color: #E8D2A7;
}

.«lang» table .code  {
    font-size: 12pt;
    color: #ffffff;
    background-color: #000311;
}

.«lang» table .jump * {
     background-color: #000311;
     color: #3a8bda;
     border-right: 0px;
}
.«lang» table .jump {
    background-color: #000311;
    color: #3a8bda;
    border-right: 0px solid white;
    border-left:  0px;
    border-top: 0px;
    border-bottom: 0px;
}


.«lang» pre {
    margin: 0px;
    padding: 0px;
    font-size: 12pt;
    font-family: monofur;
}

